<% @camps.each do |camp| %>
  <ul>
   <bold><%= camp.name %></bold>

    <div>
      <%= link_to "Edit", edit_camp_path(camp) %>
      <%= link_to "Delete", camp_path(camp), method: :delete, data: { confirm: 'Are you sure>' }  %>
